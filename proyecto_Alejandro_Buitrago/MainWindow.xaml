<Window x:Class="proyecto_Alejandro_Buitrago.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:proyecto_Alejandro_Buitrago"
        mc:Ignorable="d"
        Title="Online Woods" Height="850.462" Width="999">
    <StackPanel>
        
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Background="AliceBlue">
            <Button Content="Inicio" Margin="20" Width="100" Height="30" Click="Button_Click" Style="{StaticResource botonPrincipal}"/>
            <Button Content="Añadir" Margin="20" Width="100" Height="30" Click="Button_Click_2" Style="{StaticResource botonPrincipal}"/>
            <Button Content="Mostrar" Margin="20" Width="100" Height="30" Click="Button_Click_1" Style="{StaticResource botonPrincipal}"/>
            <Button Content="Facturas" Margin="20" Width="100" Height="30" Click="Button_Click_3" Style="{StaticResource botonPrincipal}"/>
        </StackPanel>
        <Frame x:Name="MyNavigationFrame" NavigationUIVisibility="Hidden" Height="720" Margin="10"/>
    </StackPanel>
</Window>
